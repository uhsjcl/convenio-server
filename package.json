{
  "name": "convenio-server",
  "version": "0.4.0",
  "description": "API for the convention app",
  "main": "index.ts",
  "repository": "git@github.com:uhsjcl/convenio-server.git",
  "author": "Jason Yu",
  "license": "",
  "private": true,
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "scripts": {
    "build": "gulp",
    "serve": "node build/src/index",
    "postinstall": "npx prisma introspect && npx prisma generate",
    "docker:start": "docker-compose up -d",
    "docker:start-server": "docker-compose up -d service",
    "docker:start-database": "docker-compose up -d postgres-db",
    "docker:stop": "docker-compose stop",
    "docker:stop-server": "docker-compose stop service",
    "docker:stop-database": "docker-compose stop postgres-db",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "docker:build-server": "docker-compose build server",
    "docker:build-database": "docker-compose build postgres-db",
    "dev": "ts-node-dev --project tsconfig.json --no-notify --respawn --transpileOnly src",
    "watch": "gulp watch",
    "prune-branches": "read -n 1 -s -r -p \"This command will remove all local branches not present on remote. Press any key to confirm.\n\"; git fetch -p && for branch in `git branch -vv | grep ': gone]' | awk '{print $1}'`; do git branch -D $branch; done",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/gulp": "^4.0.7",
    "@types/gulp-babel": "^6.1.29",
    "@types/merge2": "^1.3.0",
    "@types/socket.io": "^2.1.11",
    "core-js": "^3.8.0",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "nodemon": "^2.0.6",
    "regenerator-runtime": "^0.13.7",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@prisma/cli": "^2.12.1",
    "@prisma/client": "^2.12.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4",
    "merge-stream": "^2.0.0",
    "socket.io": "^3.0.3",
    "winston": "^3.3.3"
  }
}
